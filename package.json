{
  "name": "habit-api",
  "version": "1.0.0",
  "description": "An app that tracks habits and stores in the postgres database",
  "main": "app.js",
  "scripts": {
    "test": "jest --setupFiles ./__tests__/integration/config.js --watchAll --detectOpenHandles --runInBand --verbose",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "seedDev": "node dbConfig/seedDev.js",
    "unitTests": "jest --testPathPattern=/unit/ --watchAll --verbose",
    "coverage": "jest --testPathPattern=/unit/ --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/integration/config.js"
    ]
  },
  "engines": {
    "node": "12.18.4",
    "npm": "6.14.11"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
